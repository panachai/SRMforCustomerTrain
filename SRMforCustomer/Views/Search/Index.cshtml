@model List<SRMforCustomer.Models.RequestsModel>

@{
    ViewBag.Title = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@if (Model != null) {
    foreach (var req in Model) {
        <h2>@req.ReTopicName</h2>
    }
} else if (Model?.Count == 0) {
    <h2>โปรตรวจสอบ Ticket ของท่าน</h2>
}
